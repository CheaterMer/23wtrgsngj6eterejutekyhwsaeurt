local c0s = {
CFrame.new(2.2,1.5,0.5)*CFrame.Angles(0, 0, math.rad(65)),
CFrame.new(1.4,2,0.5)*CFrame.Angles(0, 0, math.rad(80)),
CFrame.new(1.6,4,0.5)*CFrame.Angles(0, 0, math.rad(110)),
CFrame.new(1.2,3,0.5)*CFrame.Angles(0, 0, math.rad(150)),
CFrame.new(-2.2,1,0.5)*CFrame.Angles(0, 0, math.rad(-65)),
CFrame.new(-1.4,2,0.5)*CFrame.Angles(0, 0, math.rad(-80)),
CFrame.new(-1.6,4,0.5)*CFrame.Angles(0, 0, math.rad(-110)),
CFrame.new(-1.2,3,0.5)*CFrame.Angles(0, 0, math.rad(-150))
}
local welds = {weld1,weld2,weld3,weld4,weld1L,weld2L,weld3L,weld4L}

--tweenanimation
for i = 1,#welds do
        local part = welds[i]
        
        local Info = TweenInfo.new(
            3,                              --Length (seconds)
            Enum.EasingStyle.Sine,          --Easing Style
            Enum.EasingDirection.InOut,       --Easing Direction
            -1,                             --Times Repeated
            true,                           --reversed
            0                               --de≈üau
            )
            
        local Goals = {
            C0 = c0s[i]
        }
        
        local wingtween = TweenService:Create(part, Info, Goals)
        wingtween:Play()
end
       
        
        
        
for i = 1,8,1 do
    spawn(function()
local args = {
   [1] = {
        [1] = getrenv()._G.Pass,
        [2] = "KnifeProjectile",
        [3] = "Spawn",
        [4] = Vector3.new(0, 3000, 0) 
    }
}

game:GetService("ReplicatedStorage").Remotes.CharaMoves:InvokeServer(unpack(args))


end)
end
 
local detect 
local detect2
local val = Instance.new('NumberValue',char)
val.Name = 'Wingpartslashcount'
local num = 0
local tab = {}
local tab2 = {}
detect = char.Attacks.ChildAdded:Connect(function(child)
    if child.Name == "KnifeSlashProjectile" then
        table.insert(tab,child)
        child:WaitForChild("ParticleEmitter"):Destroy()
        child:WaitForChild("BodyVelocity"):Destroy()
        child.Color = Color3.fromRGB(205, 0, 255)
        for i, v in pairs(child:GetChildren()) do
            if v.Name == "Burst" then
                v.Color = ColorSequence.new{
                    ColorSequenceKeypoint.new(0, child.Color),
                    ColorSequenceKeypoint.new(1, child.Color)
                }
            end
            if v.ClassName == "Trail" then
                v.Color = ColorSequence.new{
                    ColorSequenceKeypoint.new(0, child.Color),
                    ColorSequenceKeypoint.new(1, child.Color)
                }
            end
            if _G.notrails == true then
            if v.ClassName == "Trail" then
                
                v.Enabled = false
                
            end
            end
        end
local bp = Instance.new('BodyPosition',child)
bp.Name = 'Client'
bp.P = 30000
bp.D = 150
bp.Position = char.HumanoidRootPart.Position
table.insert(tab2,bp)
        num = num + 1
        val.Value = num
    end
end)
_G.nowings = false
detect2 = val:GetPropertyChangedSignal("Value"):Connect(function(amo)
if val.Value == 8 then
    
for i = 1,#tab do
spawn(function()
    local part = welds[i].Part1
    local sl = tab[i]
    local bs = tab2[i]
    part.Transparency = 1
while true do game['Run Service'].Heartbeat:wait()
    if _G.nowings then break end
    local cfr = part.CFrame*CFrame.Angles(0,math.rad(90),math.rad(180))
bs.Position = cfr.p 
sl.CFrame = cfr
end
part:Destroy()
sl:Destroy()
end)
end
detect:Disconnect()
detect2:Disconnect()
val:Destroy()
end
end)
