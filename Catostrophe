-- Gui to Lua
-- Version: 3.2

-- Instances:

local BillboardGui = Instance.new("BillboardGui")
local TextLabel = Instance.new("TextLabel")

--Properties:

BillboardGui.Parent = game:GetService("Workspace").FunnyMan.Head
BillboardGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
BillboardGui.Active = true
BillboardGui.AlwaysOnTop = true
BillboardGui.ExtentsOffset = Vector3.new(0, 5, 0)
BillboardGui.LightInfluence = 1.000
BillboardGui.Size = UDim2.new(0, 200, 0, 50)

TextLabel.Parent = BillboardGui
TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
TextLabel.BackgroundTransparency = 1.000
TextLabel.Size = UDim2.new(0, 200, 0, 50)
TextLabel.Font = Enum.Font.Arcade
TextLabel.Text = ""
TextLabel.TextColor3 = Color3.fromRGB(100, 0, 0)
TextLabel.TextSize = 30.000

-- Scripts:

local function MEDHLHN_fake_script() -- TextLabel.Script 
	local script = Instance.new('Script', TextLabel)

		local TextLabel = script.Parent	
		local Text
	
		function Sound()
			local Sound = Instance.new("Sound",workspace)
			Sound.Name = "JoeMama"
			Sound.Volume = 7
			Sound.SoundId = "rbxassetid://5093601873"
			Sound.PlaybackSpeed = 1
			Sound:Play()
			coroutine.resume(coroutine.create(function()
				wait(1)
				Sound:Destroy()
			end))
		end
	
		function funny(word, c1, c2, c3, TF)
			Text = word
			for i = 1, #Text do
				TextLabel.Text = string.sub(Text, 1, i)
				TextLabel.Font = TF
				Sound()
				TextLabel.TextColor3 = Color3.fromRGB(c1, c2, c3)
				wait(0.03)
			end
		end
		
		shake = true
	    wait(0.5)
		funny("*Now I hate everyone", 255, 0, 0, "Arcade")
		wait(3)
		funny("*So I've been thinking.", 255, 255, 255, "Arcade")
		wait(2)
		funny("*destroying everyone", 255, 255, 255, "Arcade")
		wait(2)
		funny("(Don't worry, I'll turn this world into something it didn't exist)", 255, 255, 255, "Arcade")
		wait(4)
		funny("*shall we start?", 255, 255, 255, "Arcade")
		wait(1)
		game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
		BillboardGui:Destroy()
SoundEffect()
wait(0.1)
game:GetService("Workspace").FunnyMan:Destroy()
wait(3)
game:GetService("Workspace").TeleportSound:Destroy()
end
coroutine.wrap(MEDHLHN_fake_script)()

task.spawn(function()
while true do
        wait()
        game.Players.LocalPlayer.Character.Humanoid.Health = 69420
end
end)


--// Anti-Fling v1 (very close in beta alot of stuff might break when using)

if beta == true then
task.spawn(function()
while true do
wait()
char = game.Players.LocalPlayer.Character
spawn(function()
    repeat wait()
        for _,v in pairs(char:GetChildren()) do
        if v.Name == 'KnockBack' or v.Name == "Server" then
            v:Destroy()
        end
        end
    until false
end)
end
end)
end

