

function SoundEffect()
local Sound = Instance.new("Sound",workspace)
Sound.Name = "TeleportSound"
Sound.Volume = 7
Sound.SoundId = "rbxassetid://6439289406"
Sound.PlaybackSpeed = 1
Sound:Play()
end

pStorage = game.ReplicatedStorage
player = game.Players.LocalPlayer
char = player.Character
currentCF = char.HumanoidRootPart.CFrame
for _FORV_138_, _FORV_139_ in pairs(game.Workspace:GetChildren()) do
  if _FORV_139_.Name == "xsinew" then
    dummyClone = _FORV_139_:Clone()
    dummyClone.Parent = game.Workspace
    dummyClone.Name = "FunnyMan"
    dummyClone.HumanoidRootPart.Anchored = false
    dummyClone.Torso.CanCollide = false
    dummyClone.Head.CanCollide = false
    dummyClone["Left Arm"].CanCollide = false
    dummyClone["Right Arm"].CanCollide = false
    dummyClone["Left Leg"].CanCollide = false
    dummyClone["Right Leg"].CanCollide = false
    dummyClone.HumanoidRootPart.Anchored = false
    dummyRoot = dummyClone.HumanoidRootPart
    weld = Instance.new("Weld", dummyRoot)
    weld.Part0 = dummyRoot
    weld.Part1 = char.HumanoidRootPart
    dummyRoot.Name = "dummyRoot"
    An = {
      0,
      0,
      0
    }
    Angle = CFrame.fromEulerAnglesXYZ(math.rad(An[1]), math.rad(An[2]), math.rad(An[3]))
    weld.C0 = CFrame.new(0, -1, -1.2) * Angle
  end
end
local Anim = Instance.new("Animation") Anim.AnimationId = "rbxassetid://5869935888" local k = game.Workspace.FunnyMan.Humanoid:LoadAnimation(Anim) k:Play()
game:GetService("Workspace").FunnyMan.Head.HealthBar.Frame.PName.TextStrokeColor3 = Color3.new(255,0,0)
game:GetService("Workspace").FunnyMan.Head.HealthBar.Frame.PName.TextStrokeTransparency = 0
game:GetService("Workspace").FunnyMan.Head.HealthBar.Frame.PName.TextColor3 = Color3.fromRGB(0,0,0)
game:GetService("Workspace").FunnyMan.Head.HealthBar.Frame.Health.BackgroundColor3 = Color3.fromRGB(255,0,0)
game:GetService("Workspace").FunnyMan.Head.HealthBar.Frame.PName.Text = " Chara"
