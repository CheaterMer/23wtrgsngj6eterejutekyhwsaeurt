--kill aura



local Aura = false

game:GetService("UserInputService").InputBegan:Connect(function(inp)
    if inp.KeyCode == Enum.KeyCode.KeypadOne then
	
if game.Players.LocalPlayer.PlayerGui.CharaIndicator.Indicator.Text == "Combat Enabled" then

	 
if Aura == true then
		
	local A_1 = {
		[1] = getrenv()._G.Pass,
		[2] = "Chatted",
		[3] = "Aura off",
		[4] = Color3.new(1, 0, 0)
	}
	local Event = game:GetService("ReplicatedStorage").Remotes.Events
	Event:FireServer(A_1)
print("off")
Aura = false
		
elseif Aura == false then
		
		Aura = true
		
	local A_1 = {
		[1] = getrenv()._G.Pass,
		[2] = "Chatted",
		[3] = "Aura On",
		[4] = Color3.new(1, 0, 0)
	}
	local Event = game:GetService("ReplicatedStorage").Remotes.Events
	Event:FireServer(A_1)
	
	

while Aura == true do

wait(0.2)

local player = game.Players.LocalPlayer
local char = player.Character
for _,name in pairs(game.workspace:GetChildren()) do

if name:FindFirstChildOfClass('Humanoid') then
if name:FindFirstChild('Torso') then
range = (char.PrimaryPart.Position - name.PrimaryPart.Position).Magnitude
if range <15 and name.Name ~= player.Name then
spawn(function()
local args = {
    [1] = getrenv()._G.Pass,
    [2] = name,
    [3] = {
        ["HitTime"] = 1,
        ["Type"] = "Knockback",
        ["HitEffect"] = "BoneHitEffect",
        ["CombatInv"] = true,
        ["HurtAnimation"] = game:GetService("ReplicatedStorage").Animations.HurtAnimations.Hurt1,
        ["Velocity"] = Vector3.new(0, 20, 0) + game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.lookVector * 15,
        ["Sound"] = game:GetService("ReplicatedStorage").Sounds.Kick,
        ["Damage"] = 40
    }
}

game:GetService("ReplicatedStorage").Remotes.Damage:InvokeServer(unpack(args))

end)
end

end
end
end
end
	
	
print("on")



end
end
end
end)

--nan aura

local KillAura = false

game:GetService("UserInputService").InputBegan:Connect(function(inp)
    if inp.KeyCode == Enum.KeyCode.KeypadTwo then
	
if game.Players.LocalPlayer.PlayerGui.CharaIndicator.Indicator.Text == "Combat Enabled" then

	 
if KillAura == true then
		
	local A_1 = {
		[1] = getrenv()._G.Pass,
		[2] = "Chatted",
		[3] = "...ok",
		[4] = Color3.new(1, 0, 0)
	}
	local Event = game:GetService("ReplicatedStorage").Remotes.Events
	Event:FireServer(A_1)
print("off")
KillAura = false
		
elseif KillAura == false then
		
		KillAura = true
		
	local A_1 = {
		[1] = getrenv()._G.Pass,
		[2] = "Chatted",
		[3] = "Get Out!",
		[4] = Color3.new(1, 0, 0)
	}
	local Event = game:GetService("ReplicatedStorage").Remotes.Events
	Event:FireServer(A_1)
	
	

while KillAura == true do

wait(0.2)

local player = game.Players.LocalPlayer
local char = player.Character
for _,name in pairs(game.workspace:GetChildren()) do

if name:FindFirstChildOfClass('Humanoid') then
if name:FindFirstChild('Torso') then
range = (char.PrimaryPart.Position - name.PrimaryPart.Position).Magnitude
if range <15 and name.Name ~= player.Name then
spawn(function()
local args = {
    [1] = getrenv()._G.Pass,
    [2] = name,
    [3] = {
        ["HitTime"] = 1,
        ["Type"] = "Knockback",
        ["HitEffect"] = "BoneHitEffect",
        ["CombatInv"] = true,
		["BlockAble"] = false,
        ["HurtAnimation"] = game:GetService("ReplicatedStorage").Animations.HurtAnimations.Hurt1,
        ["Velocity"] = Vector3.new(0, 30, 0) + game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.lookVector * 100,
        ["Sound"] = game:GetService("ReplicatedStorage").Sounds.Kick,
        ["Damage"] = "nan"
    }
}

game:GetService("ReplicatedStorage").Remotes.Damage:InvokeServer(unpack(args))

end)
end

end
end
end
end
	
	
print("on")



end
end
end
end)



