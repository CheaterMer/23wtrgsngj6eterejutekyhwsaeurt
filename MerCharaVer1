local player = game.Players.LocalPlayer
local char = player.Character
local humanoid = char.Humanoid
local rootpart = char.HumanoidRootPart



local uis = game:GetService("UserInputService")
local rep = game:GetService("ReplicatedStorage")
local remotes = rep:WaitForChild("Remotes")
local events = remotes:WaitForChild("Events")

local backpack = player.Backpack
local main = backpack.Main
local lockonscript = main.LockOnScript
local lock = lockonscript.LockOn
local UnLock = lockonscript.UnLock
local pass = getrenv()._G.Pass
local mouse = player:GetMouse()


local function LockCalculate(lock)
	if not lock.Value then
		return CFrame.new(mouse.Hit.p, mouse.Hit.p + rootpart.CFrame.lookVector * 5);
	end;
	return CFrame.new(lock.Value.HumanoidRootPart.Position + Spot(lock.Value, lock.Value.HumanoidRootPart.CFrame, 5).lookVector * 5, lock.Value.HumanoidRootPart.Position);
end


local CameraShaker = require(rep.ClientModules.CameraShaker)

local camera = game.Workspace.CurrentCamera

local camShake = CameraShaker.new(Enum.RenderPriority.Camera.Value, function(shakeCf)
    camera.CFrame = camera.CFrame * shakeCf
end)

camShake:Start()


--// Flashes

function FWhiteFlash()
    char = game.Players.LocalPlayer.Character
    for _,v in pairs(rep.Resources.LocalScripts:GetChildren()) do
    if v.Name == 'ShortWhiteScreen2' then
        clone = v:Clone()
        clone.Parent = char
       end
    end
end





local mouse = game.Players.LocalPlayer:GetMouse()
mouse.KeyDown:Connect(function(k) 
 if game.Players.LocalPlayer.PlayerGui.CharaIndicator.Indicator.Text == "God Enabled" then
if k == "z" then
_G.cdr = false
if _G.cdr then return end
_G.cdr = true
pass = getrenv()._G.Pass
player = game.Players.LocalPlayer
m = player:GetMouse()
local LockOn = player.Backpack.Main.LockOnScript.LockOn.Value
if LockOn then 
target = LockOn
r2 = target.HumanoidRootPart.CFrame
elseif not LockOn then
r2 = m.Hit*CFrame.new(0,4,0)
end
char = player.Character
h = char.Humanoid
a = Instance.new('Animation')
a.AnimationId = 'rbxassetid://3924265388'
k = h:LoadAnimation(a)
k:Play()
k:AdjustSpeed(1.5)
--char.HumanoidRootPart.CFrame = char.HumanoidRootPart.CFrame*CFrame.new(0,20,0)
--wait(.1)
amm = 1
--char.HumanoidRootPart.Position + Vector3.new(0,10000,0)
for i = 1,amm,1 do
spawn(function()
local A_1 =  {
      [1] = pass,
      [2] = "KnifeProjectile", 
      [3] = "Spawn", 
      [4] = r2.p
}
