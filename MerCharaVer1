local UI = game:GetObjects("rbxassetid://8524217009")[1]
if game.CoreGui:FindFirstChild("FPSCounter") then
	game.CoreGui:FindFirstChild("FPSCounter"):Destroy()
end
UI.Parent = game.CoreGui
