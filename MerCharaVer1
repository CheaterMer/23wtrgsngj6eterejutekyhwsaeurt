game.Players.LocalPlayer.PlayerGui.CharaIndicator.Indicator.Text = "Combat Enabled"

elseif game.Players.LocalPlayer.PlayerGui.CharaIndicator.Indicator.Text == "Combat Enabled" then

game.Players.LocalPlayer.PlayerGui.CharaIndicator.Indicator.Text = "Star Glitcher"


elseif game.Players.LocalPlayer.PlayerGui.CharaIndicator.Indicator.Text == "Star Glitcher" then

game.Players.LocalPlayer.PlayerGui.CharaIndicator.Indicator.Text = "Music Box"

elseif game.Players.LocalPlayer.PlayerGui.CharaIndicator.Indicator.Text == "Music Box" then

game.Players.LocalPlayer.PlayerGui.CharaIndicator.Indicator.Text = "GTChara"
end
end
end)

                      local mouse = game.Players.LocalPlayer:GetMouse()
