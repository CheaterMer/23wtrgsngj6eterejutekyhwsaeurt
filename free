	Name = "Tp Chest"
})

ATab:AddButton({
	Name = "Tp SeaKing Chest!",
	Callback = function()
	
for i,v in pairs(workspace.Island:GetChildren()) do
if v.Name == "Legacy Island1" or v.Name == "Legacy Island2" or v.Name == "Legacy Island3" or v.Name == "Legacy Island4" then
for a,j in pairs(v:GetChildren()) do
if j.Name == "ChestSpawner" then
print(j.CFrame)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = j.CFrame
end
end
end
end

end
})

ATab:AddButton({
	Name = "Tp Hydra Chest!",
	Callback = function()
	
for i,v in pairs(workspace.Island:GetChildren()) do
if v.Name == "Sea King Thunder" or v.Name == "Sea King Water" or v.Name == "Sea King Lava" then
for a,j in pairs(v:GetChildren()) do
if j.Name == "Main" then
print(j.CFrame)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = j.CFrame + Vector3.new(0,10,0)
end
end
end
end

end
})

ATab:AddButton({
	Name = "Tp Ghost Ship!",
	Callback = function()
	
for i,v in pairs(workspace.GhostMonster:GetChildren()) do
