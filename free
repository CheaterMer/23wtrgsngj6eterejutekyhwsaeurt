local DTab = Window:MakeTab({
	Name = "Misc",
	Icon = "rbxassetid://434150984",
	PremiumOnly = false
})

DTab:AddButton({
	Name = "Legacy Pose",
	Callback = function(Value)

local l__LocalPlayer__2 = game.Players.LocalPlayer;
local v3 = l__LocalPlayer__2.Character;

if not v3 then
	while true do
		wait(0.15);
		v3 = l__LocalPlayer__2.Character;
		if v3 then
			break;
		end;	
	end;
end;

local l__Humanoid__4 = v3:WaitForChild("Humanoid");

local u1 = Value
local u3 = 1;
local l__HumanoidRootPart__4 = v3:WaitForChild("HumanoidRootPart");
local l__RunService__5 = game:GetService("RunService");

	local v19 = nil;
	local v20 = nil;
	while true do
	
	
		if u3 == 1 then
			if v20 then
				v20:Destroy();
				v20 = nil;
			end;
			
			
			
			local v21 = workspace.Areas:FindFirstChild("LegacyIslandArea") or workspace.Areas:FindFirstChild("HydraIslandArea");
			if v21 then
				if not v19 then
					v19 = game.ReplicatedStorage.Chest.Etc.Arrow:Clone();
					v19.Parent = workspace.Effects;
				end;
				v19.CFrame = CFrame.new(l__HumanoidRootPart__4.Position, l__HumanoidRootPart__4.Position + (v21.Position - l__HumanoidRootPart__4.Position).Unit * Vector3.new(1, 0, 1)) * CFrame.new(0, 0, -3);
				v19.Transparency = math.abs(math.sin(tick() * 1)) * 0.5;
			end;
			
			
			
			local l__GhostMonster__22 = workspace:FindFirstChild("GhostMonster");
			if l__GhostMonster__22 and l__GhostMonster__22:FindFirstChild("Ghost Ship") then
				if not v20 then
					v20 = game.ReplicatedStorage.Chest.Etc.Arrow:Clone();
					v20.Color = Color3.fromRGB(0, 255, 0);
					v20.Parent = workspace.Effects;
				end;
				v20.CFrame = CFrame.new(l__HumanoidRootPart__4.Position, l__HumanoidRootPart__4.Position + (l__GhostMonster__22["Ghost Ship"].HumanoidRootPart.Position - l__HumanoidRootPart__4.Position).Unit * Vector3.new(1, 0, 1)) * CFrame.new(0, 0, -3);
				v20.Transparency = math.abs(math.sin(tick() * 1)) * 0.5;
			end;
			
			
		end;




		if l__Humanoid__4.Health <= 0 then
			break;
		end;
		if not u1 then
			break;
		end;
		l__RunService__5.Heartbeat:Wait();	
	end;
	if v19 then
		v19:Destroy();
	end;
	if v20 then
		v20:Destroy();
	end;
	
	

end
})

