local plr = Players.LocalPlayer

local stringToFormat = "%I:%M"
	
local result = os.date(stringToFormat)

local ServerHop = true
local PickPlayer = nil
local testtest = false
local asdfga = false

_G.Animation = Standard

function Ntfi()
local Sound = Instance.new("Sound")
Sound.Parent = game.Players.LocalPlayer.Character.Head
Sound.Volume = 5
Sound.Playing = true
Sound.Looped = false
Sound.SoundId = "rbxassetid://1008383810"
Sound.Name = "Voice"
Sound.PlaybackSpeed = 1
end

function dico()

             local url = "https://discord.com/api/webhooks/1074760583104172082/vKecyC1YLvbpr8v3iZDUPjf_V1aWmUNR4LpQx7Cd3KhZevi4yW5mzIloYB47hk58a1qm"
            local data = {
               ["content"] = "@here",
               ["embeds"] = {
                   {
                       ["title"] = "**Server Hop : **" .. "Time " .. result ,
                       ["description"] = "User : " .. game.Players.LocalPlayer.Name.." Server in``` Sea King ```"..
"!join "  ..game.Players.LocalPlayer.Name,
                       ["type"] = "rich",
                       ["color"] = tonumber(0x7269da),
                       ["image"] = {
                      -- ["url"] = "" ..tostring(game:GetService("Players").LocalPlayer.Name)
                       }
                   }
               }
            }
			

			
            local newdata = game:GetService("HttpService"):JSONEncode(data)

            local headers = {
               ["content-type"] = "application/json"
            }
            request = http_request or request or HttpPost or syn.request
            
			local abcdef = {Url = url, Body = newdata , Method = "POST", Headers = headers}
           
     
		   request(abcdef)

end

