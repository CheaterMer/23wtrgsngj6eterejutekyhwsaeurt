local mapPos
if workspace:WaitForChild("Map"):FindFirstChild("MapCFrame") then
        mapPos = workspace:WaitForChild("Map"):FindFirstChild("MapCFrame").Value.Position
    else
    task.spawn(function()
        repeat task.wait() until plr.Character
        mapPos = plr.Character.HumanoidRootPart.CFrame
    end)
end


local function createRandoms()
	local slashes = {}
	local con = plr.Character.Attacks.ChildAdded:Connect(function(ch)
		if ch.Name == "Missile" then
			ch.Name = "Random"
			ch:WaitForChild("Exploded"):Destroy()
			ch:WaitForChild("ServerVelocity")
			table.insert(slashes,ch)
		end
	end)
	taskspawn(function()
		while true do
			taskspawn(function()
				remotes.AsrielMoves:InvokeServer({pass, "AsrielBarrageMove", "Fire" })
			end)
			taskwait(0.1)
			if #slashes > 21 then break end
		end
		con:Disconnect()
	end)
	taskspawn(function()
		repeat taskwait() until #slashes >= 1
		while true do
			taskwait(.07)
			local existing = 0
			for i,v in pairs(slashes) do
				if v and v.Parent then
					v.Position = mapPos + vector(mathrand(-2100,2100),mathrand(-2100,2100),mathrand(-2100,2100))
					existing = existing + 1
				end
			end
			if existing == 0 then createRandoms() break end
		end
	end)
end



local function StarRandoms()
	local slashes = {}
	local con = plr.Character.Attacks.ChildAdded:Connect(function(ch)
		if ch.Name == "Star" then
			ch.Name = "Random"
			ch:WaitForChild("Idle"):Destroy()
			ch:WaitForChild("BodyVelocity")
			table.insert(slashes,ch)
		end
	end)
	taskspawn(function()
		while true do
			taskspawn(function()
				remotes.AsrielMoves:InvokeServer({pass, "StarBlazing", "Start" })
			end)
			taskwait(0.1)
			if #slashes > 10 then break end
		end
		con:Disconnect()
	end)
	taskspawn(function()
		repeat taskwait() until #slashes >= 1
		while true do
			taskwait(.07)
			local existing = 0
			for i,v in pairs(slashes) do
				if v and v.Parent then
					v.Position = mapPos + vector(mathrand(-2100,2100),mathrand(-2100,2100),mathrand(-2100,2100))
					existing = existing + 1
				end
			end
			if existing == 0 then createRandoms() break end
		end
	end)
end


