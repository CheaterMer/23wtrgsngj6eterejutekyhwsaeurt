local function getDummy()
	local dummy
	for i,v in pairs(workspace:GetChildren()) do
		if v.Name:match("Dummy") and not plrs:FindFirstChild(v.Name) and v.Name ~= "DodgingDummy" and v.PrimaryPart then
			dummy = v
			break
		end
	end
	return dummy
end






local function createAura()
	auraModel.PrimaryPart.CFrame = plr.Character.HumanoidRootPart.CFrame
	auraModel.Name = "a"
	auraModel.Parent = plr.Character
	local lol = 0
	local gottem = 0
	local bigslashes = {}
	local con = plr.Character.Attacks.ChildAdded:Connect(function(ch)
		if ch.Name == "BigStar" then
		    ch:WaitForChild("Idle"):Destroy()
		    ch:WaitForChild("BodyVelocity")
			
			for i,v in pairs(auraModel:GetChildren()) do
				if v:IsA("MeshPart") and v.Name ~= "a" then
					ch.Name = "a"
					v.Name = "a"
					bigslashes[ch] = v
					gottem = gottem + 1
					break
				end
			end
		end
	end)
	local canCheck = false
	taskspawn(function()
		while true do
			taskspawn(function()
				remotes.AsrielMoves:InvokeServer({pass, "StarBlazing", "Start", 1})
			end)
			taskwait(0.5)
			if gottem == 1 then break end
		end
		con:Disconnect()
		canCheck = true
	end)
	taskspawn(function()
		while auraModel and auraModel.Parent do
			local res = workspace:Raycast(plr.Character.HumanoidRootPart.Position,Vector3.new(0,0,0),params)
			local pos = plr.Character.HumanoidRootPart.Position - Vector3.new(0,0,0)
			if res and res.Position then
				pos = res.Position
			end
			auraModel.PrimaryPart.CFrame = CFrame.new(pos) * CFrame.Angles(0,math.rad(lol),0)
			lol = lol + 15
			task.wait()
			local current = 0
			for i,v in pairs(bigslashes) do
				if v and v.Parent then 
					current = current + 1
					i.CFrame = v.CFrame * CFrame.new(50.1899309158325, 2.817123413086, -50.7188339233398, 1, 0, 0, 0, 0, 1, 0, -1, 0)
				end
			end
			if canCheck and current < 1 then
				canCheck = false
				auraModel:Destroy()
				for i,v in pairs(plr.Character.Attacks:GetChildren()) do
					if v.Name == "a" then v:Destroy() end
				end
				task.wait(2)
				createAura()
				break
			end
		end
	end)
end





local function Aura()
	auraModel.PrimaryPart.CFrame = plr.Character.HumanoidRootPart.CFrame
	auraModel.Name = "test"
	auraModel.Parent = plr.Character
	local lol = 0
	local gottem = 0
	local bigslashes = {}
	local con = plr.Character.Attacks.ChildAdded:Connect(function(ch)
		if ch.Name == "Star" then
		    ch:WaitForChild("Idle"):Destroy()
		    ch:WaitForChild("BodyVelocity")
			for i,v in pairs(auraModel:GetChildren()) do
				if v:IsA("MeshPart") and v.Name ~= "test" then
					ch.Name = "test"
					v.Name = "test"
					bigslashes[ch] = v
					gottem = gottem + 1
					break
				end
			end
		end
	end)
	local canCheck = false
	taskspawn(function()
		while true do
			taskspawn(function()
				--remotes.AsrielMoves:InvokeServer({pass, "StarBlazing", "Start", 1})
			end)
			taskwait(0.5)
			if gottem == 5 then break end
		end
		con:Disconnect()
		canCheck = true
	end)
	taskspawn(function()
		while auraModel and auraModel.Parent do
			local res = workspace:Raycast(plr.Character.HumanoidRootPart.Position,Vector3.new(0,-999,0),params)
			local pos = plr.Character.HumanoidRootPart.Position - Vector3.new(0,4,0)
			if res and res.Position then
				pos = res.Position
			end
			auraModel.PrimaryPart.CFrame = CFrame.new(pos) * CFrame.Angles(0,math.rad(lol),0)
			lol = lol + 45
			task.wait()
			local current = 0
			for i,v in pairs(bigslashes) do
				if v and v.Parent then 
					current = current + 1
					i.CFrame = v.CFrame * CFrame.new(math.sin(tick())*145,2.4,0)
				end
			end
			if canCheck and current < 5 then
				canCheck = false
				auraModel:Destroy()
				for i,v in pairs(plr.Character.Attacks:GetChildren()) do
					if v.Name == "test" then v:Destroy() end
				end
				task.wait(2)
				Aura()
				break
			end
		end
	end)
end




local function test()
	auraModel.PrimaryPart.CFrame = plr.Character.HumanoidRootPart.CFrame
	auraModel.Name = "Aura"
	auraModel.Parent = plr.Character
	local lol = 0
	local gottem = 0
	local bigslashes = {}
	local con = plr.Character.Attacks.ChildAdded:Connect(function(ch)
		if ch.Name == "Missile" then
		    ch:WaitForChild("Exploded"):Destroy()
		    ch:WaitForChild("ServerVelocity")
			for i,v in pairs(auraModel:GetChildren()) do
				if v:IsA("MeshPart") and v.Name ~= "Aura" then
					ch.Name = "Aura"
					v.Name = "Aura"
					bigslashes[ch] = v
					gottem = gottem + 1
					break
				end
			end
		end
	end)
	local canCheck = false
	taskspawn(function()
		while true do
			taskspawn(function()
				remotes.AsrielMoves:InvokeServer({pass, "AsrielBarrageMove", "Fire", 1})
			end)
			taskwait(0.5)
			if gottem == 20 then break end
		end
		con:Disconnect()
		canCheck = true
	end)
	taskspawn(function()
		while auraModel and auraModel.Parent do
			local res = workspace:Raycast(plr.Character.HumanoidRootPart.Position,Vector3.new(0,0,0),params)
			local pos = plr.Character.HumanoidRootPart.Position - Vector3.new(0,4,0)
			if res and res.Position then
				pos = res.Position
			end
			auraModel.PrimaryPart.CFrame = CFrame.new(pos) * CFrame.Angles(0,math.rad(lol),0)
			lol = lol + 45
			task.wait()
			local current = 0
			for i,v in pairs(bigslashes) do
				if v and v.Parent then 
					current = current + 1
					i.CFrame = v.CFrame * CFrame.new(math.sin(tick())*145,1.4,0)
				end
			end
			if canCheck and current < 20 then
				canCheck = false
				auraModel:Destroy()
				for i,v in pairs(plr.Character.Attacks:GetChildren()) do
					if v.Name == "Aura" then v:Destroy() end
				end
				task.wait(2)
				test()
				break
			end
		end
	end)
end


local function ww()
	auraModel.PrimaryPart.CFrame = plr.Character.HumanoidRootPart.CFrame
	auraModel.Name = "ww"
	auraModel.Parent = plr.Character
	local lol = 0
	local gottem = 0
	local bigslashes = {}
