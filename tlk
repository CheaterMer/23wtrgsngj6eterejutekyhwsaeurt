--[kill aura (maybe need fix)]--

task.spawn(function()
    while task.wait() do
        if _G.forcestop then
            _G.forcestop = nil
            break
        end
        if killaura then pcall(function()
            local tool
            for i,v in pairs(attack_tools) do
                --local t = LP.Character:FindFirstChild(v) or LP.Backpack:FindFirstChild(v) or workspace:FindFirstChild(v) 
				local t = findtool(v)
                if t then
                    --t.Parent = LP.Character
                    tool = t
                    break
                end
            end
            for idx, plr in pairs(Players:GetPlayers()) do
                if plr ~= LP and CheckRange(LP.Character.HumanoidRootPart, plr.Character.HumanoidRootPart, 15) and plr.Character.Hostile.Value and not table.find(userOwner, plr.Name) and plr.Character.Humanoid.Health ~= 0 then
					--and not table.find(whitelistuser, plr.Name) 
						if LP.Team.Name == "Civilian" and plr.Team.Name ~= "Guard" then
							Remotes["Combat"]:FireServer(LP.Character, plr.Character.Head, tool)
							task.wait(0.1)
						elseif LP.Team.Name ~= "Civilian" then
							Remotes["Combat"]:FireServer(LP.Character, plr.Character.Head, tool)
							task.wait(0.1)
						end
                	end
            	end
        	end)
		end
    end
end)

--[don't try cuff me]--

task.spawn(function()
    while task.wait() do
        if donttry then pcall(function()
            for idx, plr in pairs(Players:GetPlayers()) do
                if plr ~= LP and CheckRange(LP.Character.HumanoidRootPart, plr.Character.HumanoidRootPart, 15) then
					--and not table.find(whitelistuser, plr.Name) 
						spawn(function()
							local tool
							for i,v in pairs(attack_tools) do

								for i,p in pairs(LP.Character:GetChildren()) do
									if table.find(attack_tools, p.Name) then
										return --print(p.Name, " 이미 장착되되어 있음")
									end
								end

								local t = LP.Backpack:FindFirstChild(v) 
								--or LP.Backpack:FindFirstChild(v) or workspace:FindFirstChild(v) 
								--local t = findtool(v)
								if t then
									t.Parent = LP.Character
									tool = t
									break
								end
							end
						end)
                	end
            	end
        	end)
		end
    end
end)

