local ToolRemover = Ezwin_tab:Channel("Nah i'd win")

ToolRemover:Dropdown(
    "Pick Item",
		itemlist,
    function(bool)
		print(bool)
        RemoveToollistup = bool
    end
)

ToolRemover:Button("Remove", function()
	local humanoid=game.Players.LocalPlayer.Character.Humanoid 
		for i,v in workspace:GetChildren() do
				if v:IsA("Tool") and v.Name == RemoveToollistup then
			humanoid:EquipTool(v)
		end 
	end
end)

ToolRemover:Label(" Warning This is a Broken All Tools ")

ToolRemover:Button("Remove all", function()
	local humanoid=game.Players.LocalPlayer.Character.Humanoid 
		for i,v in workspace:GetChildren() do
				if v:IsA("Tool") then
			humanoid:EquipTool(v)
		end 
	end
end)


