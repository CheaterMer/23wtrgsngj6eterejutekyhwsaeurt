for index, value in pairs(assets) do
    local res = request({
        Method="GET",
        Url=value["path"]
    })
    writefile("TLK_ASSETS/"..value["Name"], res.Body)
    repeat task.wait() until isfile("TLK_ASSETS/"..value["Name"])
end

