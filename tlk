local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Title of the library", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local target = nil
local taser = false
local unbf = false
local uncuff = false
local unhog = false
local noitem = false

local plrs = game:GetService("Players")
local plr = plrs.LocalPlayer

local names = {}
for idx, plr in pairs(game.Players:GetPlayers()) do
    table.insert(names, plr.Name)
end

function item()
if plr.Character:FindFirstChild("Taser") then
print("a")
else 
print("b")
plr.Backpack.Taser.Parent = plr.Character
end
end


function op()
local plrs = game:GetService("Players")
local plr = plrs.LocalPlayer

local player = game.Players.LocalPlayer
local char = player.Character
for _,name in pairs(game.workspace:GetChildren()) do

if name:FindFirstChildOfClass('Humanoid') then
if name:FindFirstChild('Torso') then

range = (char.Torso.Position - name.Torso.Position).Magnitude
if range < 15 and name.Name ~= player.Name then
if name.Name == targer then
plr.Backpack.Taser.Parent = plr.Character
plr.Backpack.Handcuffs.Parent = plr.Character
plr.Backpack.Grab.Parent = plr.Character
plr.Backpack.Rope.Parent = plr.Character

local part = Instance.new("Part")
part.Parent = Game.Workspace
part.Anchored = true
part.CanCollide = falsed
part.Size = Vector3.new(0.1,0.2,0.1)
part.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
part.Name = "tp"
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(name.Torso.Position)
wait(0.2)

local args = {
            [1] = "Hit",
            [2] = game:GetService("Players").LocalPlayer.Character.Taser,
            [3] = game:GetService("Players"):WaitForChild(name.Name).Character.Head
        }
        
        game:GetService("ReplicatedStorage"):WaitForChild("Tase"):FireServer(unpack(args))
        wait()
        local args = {
            [1] = "Recharge",
            [2] = game:GetService("Players").LocalPlayer.Character.Taser
        }
        
        game:GetService("ReplicatedStorage"):WaitForChild("Tase"):FireServer(unpack(args))
		
		wait(0.1)

local args = {
    [1] = game:GetService("Players").LocalPlayer.Character.Handcuffs,
    [2] = workspace:WaitForChild(name.Name)
}

game:GetService("ReplicatedStorage"):WaitForChild("IsCuffed"):FireServer(unpack(args))

wait(0.2)


local args = {
    [1] = workspace:WaitForChild(name.Name),
    [2] = "Hog"
}

game:GetService("ReplicatedStorage"):WaitForChild("IsHog"):FireServer(unpack(args))

wait(0.1)

local args = {
    [1] = workspace:WaitForChild(name.Name),
    [2] = "Grab"
}

game:GetService("ReplicatedStorage"):WaitForChild("Grab"):FireServer(unpack(args))

game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.tp.CFrame
game.Workspace.tp:Destroy()


end
end
end
end
end

end



spawn(function()
	while wait() do
	if noitem == true then
		for x, v in pairs(game.Players:GetPlayers()) do
    if v == game:GetService("Players").LocalPlayer then
    else
        local args = {
            [1] = workspace:WaitForChild(v.Name)
        }
        
        game:GetService("ReplicatedStorage"):WaitForChild("UnCuffed"):FireServer(unpack(args))
end
end
end
end
end)

spawn(function()
while wait() do
if uncuff == true then
if plrs.target.Character.IsCuffed == true then
    local args = {
        [1] = workspace:WaitForChild(target)
    }
    
    game:GetService("ReplicatedStorage"):WaitForChild("UnCuffed"):FireServer(unpack(args))
end
end
end
end
)

spawn(function()
while wait() do
if unbf == true then
if plr.Character.IsHogged == true then
    local args = {
        [1] = workspace:WaitForChild(target),
        [2] = "false"
    }
    
    game:GetService("ReplicatedStorage"):WaitForChild("BlindFold"):FireServer(unpack(args))
end
end
end
end
)

spawn(function()
	while wait() do
    wait()
    if taser == true then
		item()
        local args = {
            [1] = "Hit",
            [2] = game:GetService("Players").LocalPlayer.Character.Taser,
            [3] = game:GetService("Players"):WaitForChild(target).Character.Head
        }
        
        game:GetService("ReplicatedStorage"):WaitForChild("Tase"):FireServer(unpack(args))
        wait()
        local args = {
            [1] = "Recharge",
            [2] = game:GetService("Players").LocalPlayer.Character.Taser
        }
        
        game:GetService("ReplicatedStorage"):WaitForChild("Tase"):FireServer(unpack(args))
    end
end
end)	

local Tab = Window:MakeTab({
	Name = "main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Section"
})

Tab:AddButton({
	Name = "Get all!",
	Callback = function()
	
      		for i,v in pairs(workspace:GetChildren()) do
    if v:IsA("Tool") then
	if v == "Food" and v == "Broom" then
	print(v)
	else
	print(v,"yes")
	spawn(function()
	fireclickdetector(v.ClickDetector)
	end)
    end
	end
end

  	end    
})

Tab:AddButton({
	Name = "Skull",
	Callback = function()
	
  op()

  	end    
})

Tab:AddDropdown({
	Name = "Dropdown",
	Default = "1",
	Options = names,
	Callback = function(Value)
		print(Value)
        target = Value
	end    
})

Tab:AddToggle({
	Name = "noitem",
	Default = false,
	Callback = function(Value)
		noitem = Value

		
	end    
})

Tab:AddToggle({
	Name = "taser",
	Default = false,
	Callback = function(Value)
		taser = Value
	end    
})

Tab:AddToggle({
	Name = "uncuff",
	Default = false,
	Callback = function(Value)
		uncuff = Value
	end    
})

Tab:AddToggle({
	Name = "unbf",
	Default = false,
	Callback = function(Value)
		unbf = Value
	end    
})

OrionLib:Init()
